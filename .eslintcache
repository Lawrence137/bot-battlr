utton onClick={() => releaseBotFromArmy(bot)}>Release</button>\n          ) : (\n            <span onClick={() => addBotToArmy(bot)}></span>\n          )}\n          <span onClick={() => deleteBot(bot)}></span>\n        </div>\n      </div>\n    );\n  });\n\n  return (\n    <div className=\"ui four column grid\">\n      <div className=\"row\">\n        {/* Collection of all bots */}\n        {displayBotCards}\n      </div>\n    </div>\n  );\n}\n\nexport default BotCollection;\n","/home/lawrence/Development/Code/phase2/bot-battlr/src/components/UserBots.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":3,"column":23,"nodeType":"37","messageId":"38","endLine":3,"endColumn":34},"no-native-reassign",["39"],"no-negated-in-lhs",["40"],"no-unused-vars","'faLightning' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]